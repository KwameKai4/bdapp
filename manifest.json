{
  "name": "App",
  "description": "Description",
  "version": "1.0",
  "manifest_version": 3,

  "background": {
    "service_worker": "popup.js",
    "type": "module"
  },

  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },

  "permissions": [
    "scripting"
  ],

  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}

